<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<control xmlns="http://www.sap.com/sap.ui.library.xsd">
    <name>navpopover/FlexHandler</name>
    <derived/>
    <documentation>Communicates with the layered repository (LRep) and reacts on flexibility changes.</documentation>
    <since>1.46.0</since>
    <properties>
        <property type="sap.ui.core/object" name="initialSnapshot">
            <documentation>Initial state of all available actions. After the initial state has been set it cannot be changed again.</documentation>
        </property>
        <property type="sap.ui.core/object" name="snapshotOfUserLayer">
            <documentation>Changes made only for USER layer.</documentation>
        </property>
        <property type="sap.ui.core/object" name="snapshotOfLayersWithoutUser">
            <documentation>Changes made for all layers except USER layer.</documentation>
        </property>
    </properties>
</control>
