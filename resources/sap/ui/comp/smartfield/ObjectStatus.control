<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<control xmlns="http://www.sap.com/sap.ui.library.xsd">
    <name>smartfield/ObjectStatus</name>
    <derived/>
    <documentation>Defines a possible object status control to be rendered. The smart field may ignore the proposal.</documentation>
    <baseType>sap.ui.core/Element</baseType>
    <properties>
        <property group="Misc" type="sap.ui.core/any" name="criticality">
            <documentation>Optional attribute, which can be set, if the control type has the value ObjectStatus.</documentation>
        </property>
        <property group="Misc" defaultValue="WithIcon" type="sap.ui.comp/smartfield/CriticalityRepresentationType" name="criticalityRepresentationType">
            <documentation>Optional attribute, which can be set to control how the criticality is visualized.</documentation>
        </property>
    </properties>
</control>
