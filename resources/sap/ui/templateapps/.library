<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd" >

  <name>sap.ui.templateapps</name>
  <vendor>SAP SE</vendor>
  <copyright>SAP UI development toolkit for HTML5 (SAPUI5)

(c) Copyright 2009-2017 SAP SE. All rights reserved</copyright> <!--  NOTE: as of summer 2015, the copyright placeholder does not work here -->
  <version>1.46.0</version>

  <documentation>SAPUI5 library containing best practice apps derived from the WebIDE templates</documentation>

  <dependencies>
    <dependency>
      <libraryName>sap.ui.core</libraryName>
    </dependency>
    <dependency>
      <libraryName>sap.m</libraryName>
    </dependency>
    <dependency>
      <libraryName>sap.ui.layout</libraryName>
    </dependency>
    <dependency>
      <libraryName>sap.ushell</libraryName>
    </dependency>
  </dependencies>

  <appData>
    <!-- excludes for the JSCoverage - TODO: maintain excludes if required
    <jscoverage xmlns="http://www.sap.com/ui5/buildext/jscoverage" >
      <exclude name="sap.ui.templateapps.js." />
    </jscoverage>
    -->
    <documentation xmlns="http://www.sap.com/ui5/buildext/documentation"
        indexUrl="../../../../test-resources/sap/ui/templateapps/demokit/docuindex.json"
        resolve="lib" />
    <!-- release notes... uncomment once needed
    <releasenotes xmlns="http://www.sap.com/ui5/buildext/releasenotes"
      url="../../../../test-resources/sap/ui/templateapps/relnotes/changes-{major}.{minor}.json"
      resolve="lib" />
    -->
    <ownership xmlns="http://www.sap.com/ui5/buildext/ownership">
        <component>CA-UI5-FST</component><!-- default component for library, embedded text as a shortcut for <name>text</name> -->
    </ownership>
  </appData>

</library>
