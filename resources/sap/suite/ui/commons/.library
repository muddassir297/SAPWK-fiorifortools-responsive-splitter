<?xml version="1.0" encoding="UTF-8" ?>
<library xmlns="http://www.sap.com/sap.ui.library.xsd">

	<name>sap.suite.ui.commons</name>
	<vendor>SAP SE</vendor>
	<copyright>
		SAP UI development toolkit for HTML5 (SAPUI5)
		(c) Copyright 2009-2015 SAP SE. All rights reserved
	</copyright>
	<version>1.46.0</version>

	<documentation>SAP UI library: sap.suite.ui.commons</documentation>

	<dependencies>
		<dependency>
			<libraryName>sap.ui.core</libraryName>
		</dependency>
		<dependency>
			<libraryName>sap.suite.ui.microchart</libraryName>
		</dependency>
		<dependency>
			<libraryName>sap.m</libraryName>
		</dependency>
	</dependencies>

	<appData>
		<!-- package name for selenium -->
		<selenium xmlns="http://www.sap.com/ui5/buildext/selenium"
			package="com.sap.suite" />
		<!-- excludes for the JSDoc -->
		<jsdoc xmlns="http://www.sap.com/ui5/buildext/jsdoc">
			<exclude name="sap.suite.ui.commons.js." />
		</jsdoc>
		<!-- excludes for the JSCoverage -->
		<jscoverage xmlns="http://www.sap.com/ui5/buildext/jscoverage">
			<exclude name="sap.suite.ui.commons.js." />
		</jscoverage>
		<!-- demokit module app -->
		<documentation xmlns="http://www.sap.com/ui5/buildext/documentation"
			indexUrl="test-resources/sap/suite/ui/commons/demokit/docuindex.json" />
		<!-- Release notes files -->
		<releasenotes xmlns="http://www.sap.com/ui5/buildext/releasenotes"
			url="../../../../../test-resources/sap/suite/ui/commons/relnotes/changes-{major}.{minor}.json"
			resolve="lib" />
		<ownership xmlns="http://www.sap.com/ui5/buildext/ownership">
			<component>CA-UI5-BS-FCW</component><!-- default component for library, 
				embedded text as a shortcut for <name>text</name> -->
			<component>
				<name>CA-UI5-BS-FCP</name>
				<modules>
					<module>sap/suite/ui/commons/Timeline</module>
				</modules>
			</component>
		</ownership>

	</appData>

</library>