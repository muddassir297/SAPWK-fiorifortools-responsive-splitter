<core:FragmentDefinition
    xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1"
    xmlns="sap.m" xmlns:build="sap.build" xmlns:core="sap.ui.core"
    template:require="sap.suite.ui.generic.template.AnalyticalListPage.util.FilterUtil">

    <Button id="alp_clearFilters" text="{i18n>CLEAR_FILTERS}" type="Transparent"
	press="._templateEventHandlers.onClearPress"/>
	<Button id="alr_visualFilterFiltersButton1" text="{path:'_filter>/', formatter: 'sap.suite.ui.generic.template.AnalyticalListPage.util.FilterUtil.formatFiltersLink'}" type="Transparent"
	press="._templateEventHandlers.onFilterPress">
		<layoutData>
			<OverflowToolbarLayoutData moveToOverflow="false"/>
		</layoutData>
	</Button>
	<Button id="template::Share" icon="sap-icon://action" type="Transparent" tooltip="{i18n>SHARE}"
	press="._templateEventHandlers.onShareListReportActionButtonPress">
		<layoutData>
			<OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
		</layoutData>
	</Button>
</core:FragmentDefinition>
